package com.ohot.service.impl;

import java.util.List;
import java.util.Map;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.ohot.mapper.UsersMapper;
import com.ohot.service.UsersService;
import com.ohot.shop.vo.GoodsVO;
import com.ohot.vo.ArtistGroupVO;

@Service
public class UsersServiceImpl implements UsersService{

	@Autowired
	UsersMapper usersMapper;
	
	// 동적으로 처리한 아티스트그룹 리스트 (비회원, 회원 디엠리스트)
	@Override
	public List<ArtistGroupVO> artistGroupList(Map<String, Object> map) {
		return this.usersMapper.artistGroupList(map);
	}
	
	// 사용자가 가입한 그룹의 굿즈 상품 목록
	@Override
	public List<GoodsVO> joinGroupGoodsList(int memNo) {
		return this.usersMapper.joinGroupGoodsList(memNo);
	}
	


}
