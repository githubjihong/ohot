<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ohot.mapper.UsersMapper">

	
	
	<!-- 이메일로 사용자 찾기 -->
	<select id="findByEmail" parameterType="string" resultMap="usersMap">
	/* com.ohot.mapper.UsersMapper.findByEmail */
	    SELECT  U.USER_NO, U.USER_MAIL, USER_PSWD, A.AUTH_NM, U.ENABLED, U.SNS_YN
		FROM USERS U, USER_AUTH A
		WHERE U.USER_NO = A.USER_NO
		AND USER_MAIL = #{userMail}
	</select>
	
	<select id="findByEmailAdmin" parameterType="String" resultMap="empMap" >
	/* com.ohot.mapper.UsersMapper.findByEmailAdmin */
		SELECT 
				e.emp_no
		       , e.emp_pswd
		       , e.emp_nm
		       , e.emp_rrno
		       , e.emp_brdt
		       , e.emp_telno
		       , e.emp_eml_addr
		       , e.jncmp_ymd
		       , e.rsgntn_ymd
		       , e.emp_zip
		       , e.emp_addr
		       , e.emp_daddr
		       , e.offcs_photo
              , FN_CODE_NO_TO_CODE_NM(e.jbgd_cd) AS position
              , e.jbgd_cd
              , e.stamp_file_group_no
              , d.dept_no
              , d.dept_nm
              , d.up_dept
              , d.dept_sn
              , fg.file_group_no
              , fg.file_regdate
              , fd.file_sn
              , fd.file_original_name
              , fd.file_save_name
              , fd.file_save_locate
              , fd.file_size
              , fd.file_ext
              , fd.file_mime
              , fd.file_fancysize
              , fd.file_save_date
              , fd.file_downcount
              , ( 
                  SELECT fd.file_save_locate
                    FROM file_detail fd
                   WHERE fd.file_group_no = e.file_group_no
                ) AS profile_save_locate 
        FROM department d 
        LEFT OUTER JOIN employee e ON(e.dept_no = d.dept_no)
          LEFT OUTER JOIN file_group fg ON(e.stamp_file_group_no = fg.file_group_no)
          LEFT OUTER JOIN (
                 SELECT 
                       file_sn
                       , file_group_no
                       , file_original_name
                       , file_save_name
                       , file_save_locate
                       , file_size
                       , file_ext
                       , file_mime
                       , file_fancysize
                       , file_save_date
                       , file_downcount
                       , ROW_NUMBER() OVER (PARTITION BY file_group_no ORDER BY file_sn DESC) AS RN
                   FROM file_detail
          ) FD ON fg.file_group_no = fd.file_group_no AND FD.RN = 1
       WHERE e.rsgntn_ymd IS NULL
       AND e.emp_eml_addr = #{empEmlAddr}
	</select>
	
	<select id="findByEmailMember" parameterType="string" resultMap="memberMap">
	/* com.ohot.mapper.UsersMapper.findByEmailMember */
		SELECT    m.mem_no
		        , m.mem_last_name
		        , m.mem_first_name
		        , m.mem_nicknm
		        , m.mem_email
		        , m.mem_telno
		        , m.mem_birth
		        , m.mem_pswd
		        , m.join_ymd
		        , m.secsn_ymd
		        , m.mem_access_token
		        , m.enabled
		        , m.mem_stat_sec_code_no
		        , m.mem_sec_code_no
		        , m.mem_del_yn
		        , m.sns_mem_yn
		        , a.art_no
		        , a.art_group_no
		        , a.art_act_nm
		        , a.art_expln
		        , a.art_reg_ymd
		        , a.file_group_no
		        , a.art_del_yn
		        , fd.file_save_locate
		FROM 	member m
		LEFT OUTER JOIN artist a ON m.mem_no = a.mem_no
		LEFT OUTER JOIN (
	              SELECT 
	              		file_sn
	              		, file_group_no
	                    , file_save_locate              
	                    , ROW_NUMBER() OVER (PARTITION BY file_group_no ORDER BY file_sn DESC) AS RN
	                FROM file_detail
	       ) fd ON a.file_group_no = fd.file_group_no AND fd.RN = 1
		WHERE 	m.mem_email = #{memEmail}
	</select>
	
	<select id="selectExpMemberShipList" parameterType="int" resultMap="communityMap">
	/* com.ohot.mapper.UsersMapper.selectExpMemberShipList */
		SELECT CP.COM_PROFILE_NO
		        , CP.MEMBERSHIP_YN
		        , MS.START_YMD
		        , MS.EXP_YMD
		        , MS.MEMBERSHIP_NO
		FROM COMMUNITY_PROFILE CP
		LEFT OUTER JOIN MEMBERSHIP MS ON CP.COM_PROFILE_NO = MS.COM_PROFILE_NO
		WHERE CP.ART_GROUP_NO IN (
				    SELECT art_group_no
				    FROM community_profile
				    WHERE mem_no = #{memNo}
				      AND com_delyn = 'N'
				)
		AND CP.MEMBERSHIP_YN = 'Y'
		AND MS.EXP_YMD &lt; SYSDATE
	</select>
	
	<update id="expMemberShip" parameterType="int">
	/* com.ohot.mapper.UsersMapper.expMemberShip */
		update community_profile
		SET membership_yn = 'N'
		WHERE com_profile_no = #{comProfileNo}
	</update>
	
	<select id="getMemStopYN" parameterType="int" resultType="com.ohot.vo.ReportmanageVO">
	/* com.ohot.mapper.UsersMapper."getReportEndDt" */
		SELECT *
		FROM (
				SELECT 
				    ROW_NUMBER() OVER (ORDER BY report_end_dt DESC) AS rnum,
				    rp.report_board_no,
				    rp.report_gubun,
				    rp.report_reg_dt,
				    rp.report_end_dt
				FROM report_board_post rp
				LEFT OUTER JOIN community_post cp ON rp.report_board_no = cp.board_no
				LEFT OUTER JOIN community_reply cr ON rp.report_board_no = cr.reply_no
				WHERE rp.report_end_dt > SYSDATE
				AND (cr.mem_no=#{memNo} OR cp.mem_no =#{memNo})
			) t
		WHERE t.rnum = 1
	</select>
	
	<update id="updateMemStatus" parameterType="int">
		UPDATE member
		SET mem_stat_sec_code_no = '001'
		WHERE mem_no = #{memNo}
	</update>
	
	
	
	<!-- EMPLOYEE FULLCALENDAR -->
	<select id="getEmployeeSchedule" resultType="com.ohot.employee.vo.EmployeeScheduleVO">
		SELECT 
		    EMPLOYEE_SCHEDULE_NO
		    , START_DATE
		    , END_DATE
		    , DEPT_NO
		    , BACKGROUND_COLOR
		    , TEXT_COLOR
		    , BORDER_COLOR
		    , DEL_YN
		    , TITLE
		    , DESCRIPTION
		    , EMP_NO
		FROM EMPLOYEE_SCHEDULE
		WHERE (EMP_NO = #{empNo}
		OR DEPT_NO = (SELECT DEPT_NO
                    FROM EMPLOYEE
                    WHERE EMP_NO = #{empNo}
                 ) 
            )
		AND DEL_YN = 'N'
	</select>
	
	<insert id="addEmployeeSchedule" parameterType="com.ohot.employee.vo.EmployeeScheduleVO">
	/* com.ohot.employee.mapper.EmployeeMapper.addEmployeeSchedule */
		<selectKey resultType="int" order="BEFORE" keyProperty="employeeScheduleNo">
			SELECT NVL(MAX(employee_schedule_no), 0) + 1
			FROM  employee_schedule
		</selectKey>
		
		INSERT INTO employee_schedule (
		    employee_schedule_no
		    ,title
		    ,description
		    ,start_date
		    ,end_date
		    <if test="empNo != null and empNo != 0" >
		    ,emp_no
		    </if>
		    <if test="deptNo != null and deptNo != 0" >
		    ,dept_no
		    </if>
		    ,background_color
		    ,text_color
		    ,border_color
		) VALUES (
		    #{employeeScheduleNo}
		    , #{title}
		    , #{description}
		    , #{startDate}
		    , #{endDate}
		    <if test="empNo != null and empNo != 0" >
		    , #{empNo}
		    </if>
		    <if test="deptNo != null and deptNo != 0" >
		    , #{deptNo}
		    </if>
		    , #{backgroundColor}
		    , #{textColor}
		    , #{borderColor}
		)
	</insert>
	
	<update id="editEmployeeSchedule" parameterType="com.ohot.employee.vo.EmployeeScheduleVO">
		/* com.ohot.employee.mapper.EmployeeMapper.editEmployeeSchedule */
		UPDATE employee_schedule
			<set>
				<if test="title!=null and title!=''" >
					title = #{title},
				</if>
				<if test="description!=null and description!=''" >
					description = #{description},
				</if>
				<if test="startDate!=null and startDate!=''" >
					start_date = #{startDate},
				</if>
				<if test="endDate!=null and endDate!=''" >
					end_date = #{endDate},
				</if>
				<if test="backgroundColor!=null and backgroundColor!=''" >
					background_color = #{backgroundColor},
				</if>
				<if test="textColor!=null and textColor!=''" >
					text_color = #{textColor},
				</if>
				<if test="borderColor!=null and borderColor!=''" >
					border_color = #{borderColor},
				</if>
			</set>
		WHERE
        	employee_schedule_no = #{employeeScheduleNo}
	</update>
	
	<update id="deleteEmployeeSchedule" parameterType="int">
		UPDATE employee_schedule
		SET del_yn = 'Y'
		WHERE
        	employee_schedule_no = #{employeeScheduleNo}
	</update>
	
