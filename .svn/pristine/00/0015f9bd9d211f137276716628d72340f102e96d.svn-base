<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>
<p>memNo : ${map.memNo}</p>
<p>artGroupNo : ${map.artGroupNo}</p>
<!-- 회원번호(세션), 그룹번호, 커뮤니티프로필번호 -->
<form id="submit" action="/oho/community/fanBoardList">
	<input type="hidden" id="memNo" name="memNo" value="${map.memNo}"/> <!-- 회원 번호 (PK) -->
	<input type="hidden" name="artGroupNo" value="${map.artGroupNo}"/> <!-- 그룹 번호 (PK) -->
	<button type="button" onClick="memberCheck()">
	커뮤니티 바로가기
	</button>
</form>

<script>
<!-- 회원 여부 체크 함수 -->
function memberCheck() {
	
	const memNo = document.getElementById("memNo").value;
	console.log("memNo : ", memNo);
	
	if(!memNo) {
		alert("로그인이 필요합니다.");
	} else {
		document.getElementById("submit").submit();
	}
	
}
</script>
</body>
</html>